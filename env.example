# OpenAI API 설정
OPENAI_API_KEY=your_openai_api_key_here

# NextAuth 설정
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here

# Vercel PostgreSQL 데이터베이스 설정
POSTGRES_URL=postgresql://username:password@host:port/database

# Vercel Upstash Redis 설정
KV_URL=redis://username:password@host:port
KV_REST_API_URL=https://your-redis-rest-url.upstash.io
KV_REST_API_TOKEN=your_redis_rest_token_here
KV_REST_API_READ_ONLY_TOKEN=your_redis_readonly_token_here
REDIS_URL=redis://username:password@host:port

# Vercel 배포용
VERCEL_URL=your_vercel_url_here

# 기타 설정
NODE_ENV=development
